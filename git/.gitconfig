[color]
	ui = true
[user]
	name = Seth Hoenig
	email = shoenig@hashicorp.com
[github]
	user = shoenig
[alias]
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	st = status
	cout = checkout
	pullf = pull --ff-only
	diffs = diff --cached
	ignore = update-index --skip-worktree
	touched = diff-tree --no-commit-id --name-only -r
[core]
	editor = mg
	excludesfile = /home/hoenig/.gitignore
[merge]
	tool = mg
[push]
	default = current
